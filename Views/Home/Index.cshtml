@model IEnumerable<SistemaDeControleVeiculos.Models.VeiculosModel>

<h1>Lista de Veículos</h1>

<table class="table">
  <thead>
    <tr>
      <th>Placa</th>
      <th>Marca</th>
      <th>Modelo</th>
      <th>Ano</th>
      <th>Quilometragem</th>
    </tr>
  </thead>
  <tbody>
    @foreach(var veiculo in Model)
    {
        <tr>
            <td>@veiculo.Placa</td>
            <td>@veiculo.Marca</td>
            <td>@veiculo.Modelo</td>
            <td>@veiculo.Ano</td>
            <td>@veiculo.Quilometragem</td>
        </tr>
    }
  </tbody>
</table>
